a= input ("first number:")
b= input("second number:")
addition = int(a)+int( b)
subtraction= int(a)- int (b)
multiplication= int(a)*int(b)
division=int(a)/int (b)
print("addition")
print(addition)
print("subtraction")
print(subtraction)
print("multiplication")
print(multiplication)
print("division")
print(division)
